{
  "name": "valibot-ts-codegen",
  "description": "Generate valibot scheme from typescript type aliases",
  "version": "1.0.5",
  "main": "main.ts",
  "license": "MIT",
  "author": "Daniel Stavrov",
  "repository": {
    "type": "git",
    "url": "https://github.com/stovv/valibot-ts-codegen"
  },
  "scripts": {
    "start": "ts-node ./index.ts --path ./test/*.ts --result ./example/valibot-schemas.ts",
    "build": "tsc && node ./node_modules/shebangify/shebangify.js ./build/index.js"
  },
  "bin": {
    "vtc": "./build/index.js"
  },
  "files": [
    "./build/index.js",
    "./build/package.json",
    "index.ts",
    "tsconfig.json",
    "yarn.lock",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "dependencies": {
    "commander": "^11.1.0",
    "pkg": "^5.8.1",
    "ts-morph": "^20.0.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "commander": "^11.1.0",
    "pkg": "^5.8.1",
    "ts-morph": "^20.0.0",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.200",
    "@types/node": "^20.8.7",
    "shebangify": "^0.0.0",
    "ts-node": "^10.9.1"
  }
}
